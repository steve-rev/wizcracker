{
  "family": "wizcracker-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::807837603367.dkr.ecr.us-east-1.amazonaws.com/wizcracker-ecr",
  "containerDefinitions": [
    {
      "name": "wizcracker-container",
      "image": "placeholder/image:REPLACE_IMAGE_TAG",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
